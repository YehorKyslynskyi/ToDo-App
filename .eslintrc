{
  "plugins": [
    "react",
    "prettier"
  ],
  "extends": [
    "react-app",
    "airbnb",
    "prettier"
  ],
  "rules": {
    "no-unused-expressions": "off",
    "import/prefer-default-export": "off",
    "react/jsx-props-no-spreading": "off",
    "react/require-default-props": "off",
    "react-hooks/exhaustive-deps": "off",
    "react/prop-types": "off",
    "no-unused-vars": "warn",
    "semi": [
      "error",
      "never"
    ],
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "endOfLine": "auto"
      }
    ],
    "react/jsx-filename-extension": [
      "warn",
      {
        "extensions": [
          ".js",
          ".jsx",
          ".scss"
        ]
      }
    ]
  }
}
